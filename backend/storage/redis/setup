# Stop and disable system Redis
sudo systemctl stop redis-server
sudo systemctl disable redis-server

# Verify port 6379 is free
ss -ltnp | grep 6379

# Start your project Redis
redis-server "/mnt/e/code/Course Code/AgentSystem/AI-Trip-Planner/backend/storage/redis/redis.conf"

# Test (in another terminal)
redis-cli -h 127.0.0.1 -p 6379 ping